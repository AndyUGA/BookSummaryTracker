        <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
      


<body>
   
 
<div class="table-responsive"> 
<table class="table">
	
  <thead class = "thead-dark">
    <tr>
      <th scope="col">Current Books I'm Reading</th>


    </tr>
  </thead>


  <tbody>
  <% for(var i=0; i < result.length; i++) { %>
    <tr>
      <td>
      	<a href="/Andy/getBookSummary/<%= result[i]._id %>">
      		<%= result[i].title %>
      	</a>
      </td>
    </tr>
    <% } %>
   


  </tbody>
</table>

</div>

<div class = "noteTitle"> 
<a href="/Andy/getBookForm"> Click here to create new book summary </a>
</div>

 

  
